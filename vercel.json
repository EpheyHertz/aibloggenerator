{
    "builds": [{
        "src": "aiblog_app/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "50mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",  
            "dest": "aiblog_app/wsgi.py"
        }
    ]
}
